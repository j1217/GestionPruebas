@page "{id:int}"
@model GestionPruebas.Pages.PruebaSeleccion.EditarModel
@{
    ViewData["Title"] = "Editar Prueba";
}

<h1>@ViewData["Title"]</h1>

<form method="post">
    <input type="hidden" asp-for="PruebaEdit.ID" />

    <div class="form-group">
        <label asp-for="PruebaEdit.NombreDescripcion"></label>
        <input asp-for="PruebaEdit.NombreDescripcion" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PruebaEdit.AspiranteID "></label>
        <input asp-for="PruebaEdit.AspiranteID " class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PruebaEdit.FechaInicio "></label>
        <input asp-for="PruebaEdit.FechaInicio " class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PruebaEdit.FechaFinalizacion "></label>
        <input asp-for="PruebaEdit.FechaFinalizacion " class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PruebaEdit.TipoPrueba "></label>
        <input asp-for="PruebaEdit.TipoPrueba " class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PruebaEdit.LenguajeProgramacion "></label>
        <input asp-for="PruebaEdit.LenguajeProgramacion " class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PruebaEdit.CantidadPreguntas "></label>
        <input asp-for="PruebaEdit.CantidadPreguntas " class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PruebaEdit.Nivel "></label>
        <input asp-for="PruebaEdit.Nivel " class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PruebaEdit.EstadoPrueba "></label>
        <input asp-for="PruebaEdit.EstadoPrueba " class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">Guardar</button>
</form>
